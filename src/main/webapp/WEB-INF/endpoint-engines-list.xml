<?xml version="1.0" encoding="UTF-8"?>
<Engines>

	<Engine>
		<engine-name>nederlab</engine-name>
		<engine-type>nederlab</engine-type>
		<engine-url>https://www.nederlab.nl/api/mtas</engine-url>
		<engine-native-url-template>http://www.nederlab.nl/onderzoeksportaal/?action=results&amp;cql-expert=</engine-native-url-template>
		<tagset-conversion-table>UD2CGNNederlab</tagset-conversion-table>
		<nederlab-extra-response-fields>
			<field>NLCore_NLIdentification_editorialCode</field>
			<field>NLCore_NLIdentification_sourceRef</field>
			<field>NLTitle_yearOfPublicationMax</field>
			<field>NLTitle_textYearMax</field>
			<field>NLTitle_textQuality</field>
			<field>NLDependentTitle_subtitle</field>
		</nederlab-extra-response-fields>
		<restrict-total-number-of-results>10</restrict-total-number-of-results>
		<nederlab-query-template-location>nederlab_query_template.json</nederlab-query-template-location>
		<nederlab-document-query-template-location>nederlab_document_query_template.json</nederlab-document-query-template-location>
	</Engine>

	<Engine>
		<engine-name>opensonar</engine-name>
		<engine-type>blacklabserver</engine-type>
		<engine-url>http://172.16.10.93:8080/blacklab-server/</engine-url>
		<restrict-total-number-of-results>10</restrict-total-number-of-results>
		<engine-native-url-template>https://portal.clarin.inl.nl/opensonar_whitelab/search/simple</engine-native-url-template>
		<tagset-conversion-table>UD2CGNSonar</tagset-conversion-table>
	</Engine>

	<Engine>
		<engine-name>chn</engine-name>
		<engine-type>blacklabserver</engine-type>
		<engine-url>http://svprmc05.inl.nl/blacklab-server/</engine-url>
		<restrict-total-number-of-results>10</restrict-total-number-of-results>
		<engine-native-url-template>http://chn.inl.nl/corpus-frontend/</engine-native-url-template>
		<tagset-conversion-table>UD2CHN</tagset-conversion-table>
	</Engine>

	<Engine>
		<engine-name>zeebrieven</engine-name>
		<engine-type>blacklabserver</engine-type>
		<engine-url>http://svprmc20.ivdnt.org/blacklab-server/</engine-url>
		<engine-native-url-template>http://brievenalsbuit.inl.nl/zeebrieven/page/search</engine-native-url-template>
