		<tagset-conversion-table>UD2BaB</tagset-conversion-table>
	</Engine>

	<Engine>
		<engine-name>gysseling</engine-name>
		<engine-type>blacklabserver</engine-type>
		<engine-url>http://svprmc12.ivdnt.org/blacklab-server/Gysseling</engine-url>
		<engine-native-url-template>https://corpusgysseling.ivdnt.org/corpus-frontend/Gysseling</engine-native-url-template>
		<tagset-conversion-table>UD2GYS</tagset-conversion-table>
	</Engine>

</Engines>
